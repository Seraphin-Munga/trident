{
    "name": "trident",
    "version": "0.0.0",
    "engines": {
        "node": ">=8.9.0"
    },
    "license": "UNLICENSED",
    "cacheDirectories": [
        "node_modules"
    ],
    "husky": {
        "hooks": {
            "pre-commit": "npm run lint"
        }
    },
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "test": "ng test",
        "test-ci": "ng test --watch=false --code-coverage",
        "generate-barrels": "barrelsby --delete -d src/app -l all -q ",
        "generate-barrels-tests": "barrelsby --delete -d src/app/test-utils -l all -q",
        "build-staging": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng build --configuration=staging",
        "build-prod": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng build --configuration=production",
        "e2e": "ng e2e",
        "lint:styles": "stylelint \"src/**/*.scss\"",
        "prettier:format": "prettier --write \"{,src/**/}*.{md,json,ts,css,scss}\"",
        "prettier:lint": "prettier --check \"{,src/**/}*.{md,json,ts,css,scss}\"",
        "lint:fix": "ng lint --fix",
        "lint": "ng lint --format=stylish && npm run lint:styles"
    },
    "private": true,
    "dependencies": {
        "@angular-devkit/schematics": "11.0.7",
        "@angular/animations": "10.2.0",
        "@angular/common": "10.2.0",
        "@angular/compiler": "10.2.0",
        "@angular/core": "10.2.0",
        "@angular/forms": "10.2.0",
        "@angular/platform-browser": "10.2.0",
        "@angular/platform-browser-dynamic": "10.2.0",
        "@angular/router": "10.2.0",
        "@ant-design/icons-angular": "11.0.1",
        "@ngrx/store": "11.0.1",
        "class-transformer": "0.4.0",
        "eslint-plugin-react": "7.22.0",
        "ng-zorro-antd": "10.2.2",
        "ngx-webstorage": "7.0.1",
        "rxjs": "6.6.0",
        "tslib": "2.0.0",
        "zone.js": "0.10.2"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "0.1002.0",
        "@angular-eslint/builder": "1.1.0",
        "@angular-eslint/eslint-plugin": "1.1.0",
        "@angular-eslint/eslint-plugin-template": "1.1.0",
        "@angular-eslint/schematics": "1.1.0",
        "@angular-eslint/template-parser": "1.1.0",
        "@angular/cli": "10.2.0",
        "@angular/compiler-cli": "10.2.0",
        "@types/jasmine": "3.5.0",
        "@types/jasminewd2": "2.0.3",
        "@types/node": "12.11.1",
        "@typescript-eslint/eslint-plugin": "4.3.0",
        "@typescript-eslint/parser": "4.3.0",
        "barrelsby": "2.2.0",
        "codelyzer": "6.0.0",
        "eslint": "7.6.0",
        "eslint-config": "0.3.0",
        "eslint-plugin-import": "2.22.1",
        "eslint-plugin-jsdoc": "30.7.6",
        "eslint-plugin-prefer-arrow": "1.2.2",
        "eslint-plugin-prettier": "3.3.1",
        "eslint-plugin-unicorn": "26.0.1",
        "husky": "4.3.8",
        "jasmine-core": "3.6.0",
        "jasmine-spec-reporter": "5.0.0",
        "karma": "5.0.0",
        "karma-chrome-launcher": "3.1.0",
        "karma-coverage-istanbul-reporter": "3.0.2",
        "karma-jasmine": "4.0.0",
        "karma-jasmine-html-reporter": "1.5.0",
        "karma-junit-reporter": "2.0.1",
        "prettier": "2.2.1",
        "prettier-eslint": "12.0.0",
        "protractor": "7.0.0",
        "puppeteer": "5.5.0",
        "stylelint": "13.8.0",
        "stylelint-config-prettier": "8.0.2",
        "stylelint-config-recommended": "3.0.0",
        "stylelint-config-standard": "20.0.0",
        "stylelint-prettier": "1.1.2",
        "stylelint-scss": "3.18.0",
        "ts-node": "8.3.0",
        "typescript": "4.0.2"
    }
}
